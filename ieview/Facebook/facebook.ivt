<!--File-->
    <div class="msg-outer">
      <div class="msg-top">
        <div class="avatar">
        </div>
        <div class="user-info">
          <span class="nick">%name%</span><br>
          <span class="time">%date%, %time%</span>
        </div>
      </div>
      <div class="msg-middle">
        <div class="message icon file">
          <span class="nick">Filetransfer: </span> <span class="info-txt">%text%</span>
        </div>
        <script>
            $('.msg-outer:last').fadeIn();
            $('div.avatar:last').showAvatar('%avatar%');
        </script>
      </div>
