  <body>
    
<!--hMessageIn-->
    <div class="msg-outer">
      <div class="msg-top">
        <div class="avatar">
        </div>
        <div class="user-info">
          <span class="nick">%name%</span><br>
          <span class="time">%date%</span>
        </div>
      </div>
      <div class="msg-middle history">
        <div class="time">%time%</div>
        <div class="message">%text%</div>
      </div>
      <script>
        $('.msg-outer:last').fadeIn();
        $('.message:last').findAttachment();
        $('div.avatar:last').showAvatar('%avatar%');
      </script>
    </div>

