<!--URL-->
    <div class="msg-outer url icon">
      <span class="nick">%name%:</span> <span class="info-txt">%text%</span>
      <script>$('.msg-outer:last').fadeIn();</script>
    </div>

<!--hURL-->
    <div class="msg-outer url icon history">
      <span class="nick">%name%:</span> <span class="info-txt">%text%</span>
      <script>$('.msg-outer:last').fadeIn();</script>
    </div>

<!--Status-->
    <script>
    $.fn.writeStatus({
        name: '%name%',
        text: '%text%'
    });
    </script>

<!--hStatus-->
    <script>
    $.fn.writeStatus({
        name: '%name%',
        text: '%text%',
        history: 'history'
    });    
    </script>


